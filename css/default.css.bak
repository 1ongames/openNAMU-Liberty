body {
    font-size: 0.95rem;
    font-family: "Noto Sans Korean", "Noto Sans", "Malgun Gothic", "맑은 고딕", sans-serif;
}

ol, ul {
    margin: 0;
    padding: 0;
}

h1, h2, h3, h4, h5, h6 {
    margin: 0;
    font-family: "Noto Sans Korean", "Noto Sans", "Malgun Gothic", "맑은 고딕", sans-serif;
}

h1 {
    font-size: 2.2rem;
}

h2 {
    font-size: 2rem;
}

h3 {
    font-size: 1.8rem;
}

h4 {
    font-size: 1.6rem;
}

h5 {
    font-size: 1.4rem;
}

h6 {
    font-size: 1.2rem;
}

button {
    padding: 0;
}

input[type=search] {
    -webkit-appearance:none;
    -webkit-border-radius: 0;
    -webkit-padding-after: 0;
    -webkit-padding-before: 0;
}

img {
    border: 0;
    vertical-align: middle;
}

.Liberty {
    background-color: #f5f5f5;
}

.Liberty .nav-wrapper {
    background-color: #4188f1;
    z-index: 1001;
    box-shadow: 0 2px 4px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12);
}

.Liberty.width-size .navbar, .Liberty.width-size .content-wrapper {
    max-width: 1200px;
    margin: 0 auto;
}

.Liberty.width-size .navbar {
    min-height: 30px;
    border: 0;
}

.Liberty.width-size .content-wrapper {
    margin-top: 3.27rem;
}

.Liberty .nav-wrapper .navbar-header {
    float: left;
}

.Liberty .nav-wrapper .navbar-brand {
    height: 2.73rem;
    width: 7rem;
    background: transparent url("../img/logo.png") no-repeat scroll left center/auto 1.9rem;
    padding: 0;
    margin: 0;
    margin-left: 1rem;
}

.Liberty .nav-wrapper .navbar-nav {
    float: left;
    margin: 0;
}

.Liberty .navgation .navbar-nav>ul, .Liberty .navgation .navbar-nav>li {
    float: left;
}

.Liberty .nav-wrapper .nav-link {
    color: #fff;
}

.Liberty .nav-wrapper .fa {
    margin-right: 0.5rem;
    font-size: 1rem;
}

.Liberty .nav-wrapper .nav-link:hover, .Liberty .nav-wrapper .nav-link:focus {
    background-color: #71a5f4;
    transition: 0.3s;
}

.navbar-nav>li>a {
    padding: 0.7rem 0.8rem;
    font-size: 1rem;
}

.Liberty .nav-wrapper {
    float: left;
    text-align: left;
}

.Liberty .nav-wrapper .portlet {
    padding: 0.3rem 0;
    float: right;
    position: relative;
    margin-right: 0.3rem;
}

.Liberty .nav-wrapper .search-form {
    display: block;
    box-sizing: border-box;
    width: 14rem;
    height: 2rem;
    padding: 0.2rem 4rem 0.2rem 6px;
    font-size: 0.8rem;
    line-height: inherit;
    color: #292f33;
    background-color: #f5f8fa;
    border: 1px solid #126aed;
    border-radius: 5px;
    position:relative;
}

.Liberty .nav-wrapper .search-icon {
    position: absolute;
    top: 0.6rem;
    right: 2.5rem;
    height: 17px;
    display: block;
    cursor: pointer;
}

.Liberty .nav-wrapper .go-icon {
    position: absolute;
    top: 0.6rem;
    right: 0.8rem;
    height: 17px;
    display: block;
    cursor: pointer;
}

.Liberty .nav-wrapper .pBody .fa {
    width: auto;
    height: 17px;
    line-height: 0;
    background-color: transparent;
    border: 0;
    color: #4f5b63;
    font-size: 1.1rem;
    margin: 0;
}

.Liberty .nav-wrapper .visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}

.Liberty .nav-wrapper .nav-login {
    float: right;
    margin-right: 1rem;
}

.Liberty .nav-wrapper .nav-login .fa {
    color: #fff;
    font-size: 1.5rem;
    margin-left: 0.3rem;
    margin-right: 0;
    padding: 0.6rem 0;
}

.Liberty .nav-wrapper .nav-login .profile-link {
    display: block;
    float: left;
    padding: 0.3rem 0;
    margin: 0 0.3rem;
}

.Liberty .nav-wrapper .nav-login .profile-img {
    width: 2rem;
    height: 2rem;
    border-radius: 5px;
    margin: 0;
    border: 1px solid #126aed;
}

.Liberty .nav-wrapper .nav-login .login-menu {
    right: 0;
    left: auto;
    border-color: #e1e8ed;
    top: 120%;
}

.Liberty .nav-wrapper .nav-login .login-menu:before, .Liberty .nav-wrapper .nav-login .login-menu:after {
    bottom: 100%;
    border: solid transparent;
    content: " ";
    position: absolute;
    left: 0;
    right: auto;
}

.Liberty .nav-wrapper .nav-login .login-menu:before {
    border-bottom-color: #e1e8ed;
    border-width: 0.8rem;
    left: auto;
    right: 2.2rem;
}

.Liberty .nav-wrapper .nav-login .login-menu:after {
    border-bottom-color: #fff;
    border-width: 0.78rem;
    left: auto;
    right: 2.2rem;
}

.Liberty .nav-wrapper .nav-login .mobile-menu {
    display: none;
}

.Liberty .liberty-content {
    margin-right: 16rem;
    padding: 0rem;
}

.Liberty .liberty-sidebar {
    float: right;
    width: 15rem;
}

.Liberty .liberty-sidebar .liberty-right-fixed {
    position: fixed;
    width: 15rem;
    background-color: #fff;
}

.Liberty .liberty-content .liberty-content-header {
    border: 1px solid #e1e8ed;
    border-radius: 5px 5px 0px 0px;
    background-color: #f5f8fa;
    /*padding: 1rem;*/
}

.Liberty .liberty-content .liberty-content-header .content-tools {
    float: right;
    padding-right: 1rem;
    padding-top: 1rem;
}

.Liberty .liberty-content .liberty-content-header .content-tools .tools-btn {
    background-image: none;
    border-color: #e1e8ed;
    text-shadow: none;
    box-shadow: none;
}

.Liberty .liberty-content .liberty-content-header .content-tools .tools-btn:hover, .Liberty .liberty-content .liberty-content-header .content-tools .tools-btn:focus {
    background-color: #4188f1;
    color: #fff;
    transition: 0.3s;
}

.Liberty .liberty-content .liberty-content-header .title {
    padding: 1rem;
}

.Liberty .liberty-content .liberty-content-header .title>h1 {
    color: #000;
    font-weight: bold;
}

.Liberty .liberty-content .liberty-content-main {
    border-left: 1px solid #e1e8ed;
    border-right: 1px solid #e1e8ed;
    background-color: #fff;
    padding: 1rem;
}

.Liberty .liberty-footer {
    border: 1px solid #e1e8ed;
    border-radius: 0px 0px 5px 5px;
    background-color: #fff;
    padding: 1rem;
}

.Liberty .liberty-footer ul {
    list-style-type: none;
    list-style-image: none;
}

.Liberty .liberty-footer ul>li {
    text-align: center;
}

[tabindex="-1"]:focus {
    outline: none;
}

.none-outline, .none-outline:focus, .none-outline:hover {
    outline: none;
}

.modal-login .modal-title {
    text-align: center;
}

.modal-login .modal-dialog {
    width: 25rem;
}

.modal-login .modal-dialog .modal-header {
    padding: 0.7rem;
}

.modal-login .alert {
    margin-bottom: 0.7rem;
}

.modal-login .alert-hidden {
    display: none;
}

.modal-loginform {
    width: 20rem;
    margin: 0 auto;
}

.modal-loginform .form-field {
    display: block;
    margin: 0 0 1rem;
    padding: 0;
    width: 100%;
}

.modal-loginform .form-control {
    margin-bottom: 0.7rem;
}

.modal-loginform .modal-checkbox:not(#noop) {
    position: relative;
    display: table;
}

.modal-loginform .modal-checkbox {
    vertical-align: middle;
    margin-bottom: 0.7rem;
}

.modal-loginform .modal-checkbox:not(#noop) input[type="checkbox"] {
    opacity: 0;
    width: 2em;
    height: 2em;
    max-width: none;
    margin: 0;
    margin-right: 0.4em;
    display: table-cell;
}

.modal-loginform .modal-checkbox:not(#noop) * {
    vertical-align: middle;
    font: inherit;
}

.modal-loginform .modal-checkbox:not(#noop) input[type="checkbox"]+label {
    display: table-cell;
}

.modal-loginform .modal-checkbox:not(#noop) input[type="checkbox"]+label::before {
    -webkit-transition: 0.2s cubic-bezier(0.175,0.885,0.32,1.275);
    -moz-transition: 0.2s cubic-bezier(0.175,0.885,0.32,1.275);
    -o-transition: 0.2s cubic-bezier(0.175,0.885,0.32,1.275);
    transition: 0.2s cubic-bezier(0.175,0.885,0.32,1.275);
    content: '';
    cursor: pointer;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    position: absolute;
    left: 0;
    border-radius: 2px;
    width: 2em;
    height: 2em;
    line-height: 2em;
    border: 1px solid #777;
    top: 50%;
    margin-top: -1em;
}

.modal-loginform .modal-checkbox:not(#noop) input[type="checkbox"]+label:hover::before {
    border: 3px solid #777;
}

.modal-loginform .modal-checkbox:not(#noop) input[type="checkbox"]:checked+label::before {
    border: 3px solid #777;
    background-image: linear-gradient(transparent,transparent),url(data:image/svg+xml,%3C%3Fxml%20version%3D%221.0%22%20encoding%3D%22UTF-8%22%3F%3E%3Csvg%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%20width%3D%2224%22%20height%3D%2224%22%3E%3Cpath%20d%3D%22M4%2012l5%205L20%205%22%20stroke%3D%22%2300B78C%22%20stroke-width%3D%223%22%20fill%3D%22none%22%2F%3E%3C%2Fsvg%3E%0A);
}

.modal-loginform .btn {
    font-size: 1rem;
    padding: 0.5rem;
    margin-bottom: 0.7rem;
}

.dropdown-menu>li>a:hover {
    transition: 0.3s;
    background-image: none;
    background-color: #5997f3;
    color: #fff;
}

.dropdown-toggle-fix:after {
    display: inline-block;
    width: 0;
    height: 0;
    margin-left: .35rem;
    vertical-align: middle;
    content: "";
    border-top: .3em solid;
    border-right: .3em solid transparent;
    border-left: .3em solid transparent;
}

.dropdown-right {
    right: 0;
    left: auto;
}

.nav .open>a, .nav .open>a:focus, .nav .open>a:hover {
    background-color: transparent;
    border-color: transparent;
    transition: 0.3s;
}